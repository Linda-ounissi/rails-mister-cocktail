<p id="notice"><%= notice %></p>

<h1>Your's Cocktails</h1>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cocktails.each do |cocktail| %>
      <tr>
        <td><%= cocktail.name %></td>
        <td><%= link_to 'show' , cocktail %></td>
        <td><%= link_to 'Create a new cocktail' , new_cocktail_dose_path %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
